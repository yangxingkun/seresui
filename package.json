    "stylelint-prettier": "^3.0.0",
    "sucrase": "^3.35.0",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^4.9.5",
    "unplugin-vue-define-options": "^1.4.3",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.8.3",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
