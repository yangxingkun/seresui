{
  "name": "seresui",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "private": "true",
  "scripts": {
    "dev": "vite play",
    "build": "pnpm -r --filter=./packages/* run build",
    "build:utils": "pnpm -r --filter=./packages/utils run build",
    "build:seres_ui": "gulp -f packages/components/script/build/index.ts",
    "publish:seres_ui": "gulp -f packages/components/script/publish/index.ts",
    "auto:publish": "sh publish.sh",
    "docs:dev": "pnpm run --filter site docs:dev",
    "docs:build": "pnpm run --filter site docs:build",
    "docs:deploy": "pnpm run --filter site docs:deploy",
    "release": "release-it",
    "lint:script": "eslint --ext .js,.jsx,.vue,.ts,.tsx --fix --quiet ./",
    "lint:style": "stylelint --fix  \"packages/components/src/**/*.{css,less,scss}\"",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/gulp": "^4.0.10",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-less": "^0.0.32",
    "@types/gulp-sass": "^5.0.4",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/runtime-core": "^3.4.21",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "git-cz": "^4.9.0",
    "gulp": "^5.0.0",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-less": "^5.0.0",
    "gulp-sass": "^5.1.0",
    "happy-dom": "^8.9.0",
    "husky": "^9.0.11",
    "less": "^4.1.3",
    "lint-staged": "^15.2.2",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.7",
    "release-it": "^15.6.0",
    "sass": "^1.75.0",
    "stylelint": "^15.3.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^31.0.0",
